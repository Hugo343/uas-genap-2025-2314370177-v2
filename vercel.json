{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "public/assets/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "public/js/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "public/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array",
        "FILESYSTEM_ROOT": "/tmp"
    },
    "build": {
        "env": {
            "COMPOSER_MIRROR_PATH_REPOS": "1"
        }
    }
}